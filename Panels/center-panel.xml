<PropertyList>

 <name>777-200 Main Panel</name>
 <background>Aircraft/777-200/Panels/transparent-bg.rgb</background>
 <w>1024</w>
 <h>512</h>

<instruments>

 <!-- AP FD controls -->  
  <instrument include="Ap_Fd.xml">
    <name>AP-FD controls</name>
    <x>540</x>
    <y>456</y>
    <w>670</w>
    <h>114</h>
  </instrument>
