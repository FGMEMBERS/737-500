<instrument>
  <name>panel hotspots</name>
  <x>512</x>
  <y>256</y>
  <w>1024</w>
  <h>512</h>
  <w-base>1024</w-base>
  <h-base>512</h-base>

 <actions> 
 
<!-- standby altimeter-->
<action>
   <name>calibrate fine</name>
   <button>0</button>
   <x>-125</x>
   <y>-62</y>
   <w>12</w>
   <h>12</h>
   <binding>
    <command>property-adjust</command>
    <property>/instrumentation/altimeter/setting-inhg</property>
    <step>-0.01</step>
    <min>26.0</min>
    <max>33.0</max>
    <wrap>false</wrap>
   </binding>
  </action>

  <action>
   <name>calibrate coarse</name>
   <button>1</button>
   <x>-125</x>
   <y>-62</y>
   <w>12</w>
   <h>12</h>
   <binding>
    <command>property-adjust</command>
    <property>/instrumentation/altimeter/setting-inhg</property>
    <step>-0.1</step>
    <min>26.0</min>
    <max>33.0</max>
    <wrap>false</wrap>
   </binding>
  </action>

  <action>
   <name>calibrate fine</name>
   <button>0</button>
   <x>-113</x>
   <y>-62</y>
   <w>12</w>
   <h>12</h>
   <binding>
    <command>property-adjust</command>
    <property>/instrumentation/altimeter/setting-inhg</property>
    <step>0.01</step>
    <min>26.0</min>
    <max>33.0</max>
    <wrap>false</wrap>
   </binding>
  </action>

  <action>
   <name>calibrate coarse</name>
   <button>1</button>
   <x>-113</x>
   <y>-62</y>
   <w>12</w>
   <h>12</h>
   <binding>
    <command>property-adjust</command>
    <property>/instrumentation/altimeter/setting-inhg</property>
    <step>0.1</step>
    <min>26.0</min>
    <max>33.0</max>
    <wrap>false</wrap>
   </binding>
  </action>

</actions>
</instrument>

</instruments>

</PropertyList>
