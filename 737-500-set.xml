            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[1]"/>
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">300.0</max-lb>
      </weight>
      <weight>
            <name type="string">Cabin Zone 0a</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[2]"/>
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">4000</max-lb>
      </weight>
      <weight>
            <name type="string">Cabin Zone 0b</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[3]"/>
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">4800</max-lb>
      </weight>
      <weight>
            <name type="string">Cabin Zone 0c</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[4]"/>
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">6800</max-lb>
      </weight>
      <weight>
            <name type="string">Cabin Zone 0d</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[5]"/>
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">6000</max-lb>
      </weight>
      <weight>
            <name type="string">Cabin Zone 0e</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[6]"/>
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">3600</max-lb>
      </weight>
      <weight>
            <name type="string">Cargo 1</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[7]"/>
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">1526</max-lb>
      </weight>
      <weight>
            <name type="string">Cargo 2</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[8]"/>
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">776</max-lb>
      </weight>
      <weight>
            <name type="string">Cargo 3</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[9]"/>
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">2263</max-lb>
      </weight>
      <weight>
            <name type="string">Cargo 4</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[10]"/>
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">4674</max-lb>
      </weight>
      <weight>
            <name type="string">Cargo 5</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[11]"/>
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">414</max-lb>
      </weight>
      <weight>
            <name type="string">Cargo 6</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[12]"/>
            <min-lb type="double">0.0</min-lb>
            <max-lb type="double">718</max-lb>
      </weight>
</payload>

<nasal>
      <Boeing-737-500>
            <file>Aircraft/737-500/Nasal/liveries.nas</file>
            <file>Aircraft/737-500/Nasal/contrail.nas</file>
      </Boeing-737-500>
      <reversethrust>
            <file>Aircraft/737-500/Nasal/reverse-thrust.nas</file>
      </reversethrust>
      <airground>
            <file>Aircraft/737-500/Systems/air-ground.nas</file>
            <file>Aircraft/737-500/Systems/speedbrakevalve.nas</file>
      </airground>
      <tyresmoke>
            <file>Aircraft/737-500/Nasal/tyresmoke.nas</file>
      </tyresmoke>
</nasal>

</PropertyList>
