<PropertyList>

 <name>Aero data</name>
 
  <w-base>128</w-base>
  <h-base>256</h-base>
   
   <layers>

    <layer>
     <name>background</name>
     <texture>   
     <path>Aircraft/737-500/Instruments/Textures/737-engines.rgb</path>
     <x1>0.0</x1>
     <y1>0.0</y1>
     <x2>1.0</x2>
     <y2>1.0</y2>
     </texture>
     <w>128</w>
     <h>256</h>
    </layer>
